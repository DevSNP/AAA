.loadingmodal {
    position: fixed;
    z-index: 9999;
    height: 100%;
    width: 100%;
    top: 0px;
    background-color: Black;
    filter: alpha(opacity=60);
    opacity: 0.9;
    -moz-opacity: 0.9;
    left: 0px;
}

.loadingcenter {
    z-index: 1000;
    margin: 350px auto;
    padding: 10px;
    width: 150px;
    background-color: White;
    border-radius: 10px;
    filter: alpha(opacity=100);
    /*opacity: 1;*/
