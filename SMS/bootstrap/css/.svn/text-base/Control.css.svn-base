arrow_boxRight {
    position: relative;
    background: #ffffff;
    border: 1px solid #c3c3c3;
    width: 70%;
    float: right;
    margin: 5px 0px;
    padding: 10px;
    border-radius: 3px;
}

.arrow_boxRight:after, .arrow_boxRight:before {
    left: 100%;
    top: 35%;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
}

.arrow_boxRight:after {
    border-color: rgba(255, 255, 255, 0);
    border-left-color: #ffffff;
    border-width: 5px;
    margin-top: -10px;
}

.arrow_boxRight:before {
    border-color: rgba(68, 68, 68, 0);
    border-left-color: #c3c3c3;
    border-width: 7px;
    margin-top: -12px;
}

